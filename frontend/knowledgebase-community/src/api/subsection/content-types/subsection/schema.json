{
  "kind": "collectionType",
  "collectionName": "subsections",
  "info": {
    "singularName": "subsection",
    "pluralName": "subsections",
    "displayName": "Subsection"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "sectionId": {
      "type": "string",
      "required": true
    },
    "parentSection": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category"
    },
    "information_status": {
      "type": "enumeration",
      "enum": [
        "active",
        "temporary",
        "archived"
      ]
    },
    "description": {
      "type": "string"
    }
  }
}
